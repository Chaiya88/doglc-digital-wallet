# DOGLC Digital Wallet Orchestrator Environment Variables

# Required: API key for admin endpoints authentication
ORCHESTRATOR_API_KEY=your_secure_orchestrator_api_key_here

# Optional: Webhook URLs for alert notifications
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR/DISCORD/WEBHOOK

# Environment identifier
ENVIRONMENT=development

# Service URLs (for external health checks)
MAIN_BOT_URL=https://doglc-digital-wallet-main-bot.your-account.workers.dev
API_URL=https://doglc-digital-wallet-api.your-account.workers.dev
BANKING_URL=https://doglc-digital-wallet-banking.your-account.workers.dev
SECURITY_URL=https://doglc-digital-wallet-security.your-account.workers.dev
FRONTEND_URL=https://doglc-digital-wallet-frontend.your-account.workers.dev
ANALYTICS_URL=https://doglc-digital-wallet-analytics.your-account.workers.dev

# Alert thresholds
ERROR_RATE_THRESHOLD=5
RESPONSE_TIME_THRESHOLD=5000
HEALTH_CHECK_INTERVAL=30000

# Development mode settings
DEV_MODE=true
DEV_TOKEN=dev_orchestrator_token_123